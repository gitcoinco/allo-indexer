{
  "name": "gitcoin-indexer",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/src/indexer/index.js",
  "types": "dist/src/indexer/index.d.ts",
  "scripts": {
    "start": "./init.sh server",
    "start:dev": "concurrently -P --kill-others -n \"tsc,http,indexer\" \"npm:build:dev\" \"npm:serve:dev\" \"nodemon --delay 2 --watch dist --exec 'npm run index -- --clear {@}'\" --",
    "start:dev:mainnet": "npm run start:dev -- --chain=mainnet",
    "start:dev:goerli": "npm run start:dev -- --chain=goerli",
    "build": "tsc",
    "build:dev": "tsc --watch",
    "lint": "eslint src",
    "format": "prettier --write src",
    "test": "npx vitest run --reporter verbose",
    "test:dev": "npx vitest watch --reporter verbose",
    "serve": "node dist/src/http/server.js",
    "serve:dev": "nodemon --delay 2 --watch dist --exec 'npm run serve'",
    "passport": "node dist/src/cli/passport.js",
    "index": "node dist/src/cli/indexer.js"
  },
  "imports": {
    "#abis/*": {
      "default": "./src/indexer/abis/*"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^7.51.0",
    "chainsauce": "^1.0.18",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "multer": "^1.4.5-lts.1",
    "pluralistic": "github:gitcoinco/pluralistic.js#e47a9647c049309a9e66c1373943e42cabae3bd9",
    "serve-index": "^1.9.1",
    "statuses-bitmap": "github:gitcoinco/statuses-bitmap#3d8fd370f209ccbaffd3781cf2b6d2895237c21c",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/memory-cache": "^0.2.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.3",
    "@types/serve-index": "^1.9.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.36.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "vitest": "^0.30.1"
  }
}
