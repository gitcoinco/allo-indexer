{
  "name": "gitcoin-indexer",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/src/indexer/index.js",
  "types": "dist/src/indexer/index.d.ts",
  "scripts": {
    "start": "./init.sh server",
    "build": "tsc",
    "lint": "eslint src",
    "format": "prettier --write src",
    "dev": "tsc --watch",
    "test": "npx vitest run --reporter verbose",
    "test:watch": "npx vitest watch --reporter verbose",
    "serve": "node dist/src/http/index.js",
    "dev:serve": "nodemon --watch dist --exec 'npm run serve'",
    "passport": "node dist/src/cli/passport.js",
    "index:goerli": "node dist/src/cli/indexer.js goerli",
    "index:mainnet": "node dist/src/cli/indexer.js mainnet",
    "index:optimism": "node dist/src/cli/indexer.js optimism",
    "index:fantom": "node dist/src/cli/indexer.js fantom",
    "dev:index:goerli": "nodemon --watch dist --exec 'npm run index:goerli -- --clear'",
    "dev:index:mainnet": "nodemon --watch dist --exec 'npm run index:mainnet -- --clear'"
  },
  "imports": {
    "#abis/*": {
      "default": "./src/indexer/abis/*"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chainsauce": "^1.0.17",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "pluralistic": "github:gitcoinco/pluralistic.js#431f4fe2d429f65f2de03bdfcc66bba451da2ba7",
    "serve-index": "^1.9.1",
    "statuses-bitmap": "github:gitcoinco/statuses-bitmap#3d8fd370f209ccbaffd3781cf2b6d2895237c21c",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^18.15.3",
    "@types/serve-index": "^1.9.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "vitest": "^0.30.1"
  }
}
